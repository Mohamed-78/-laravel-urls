{
    "name": "mdev/laravel-urls",
    "description": "Generate a dynamic Url in the laravel blade page",
    "keywords": [
        "mdev",
        "laravel-urls",
        "url"
    ],
    "require": {
        "php": "^7.4|^8.0",
        "another-vendor/package": "1.*",
        "vendor/package": "1.3.2",
        "vendor/package2": "1.*",
        "vendor/package3": "^2.0.3",
        "illuminate/support": "^8.0"
    },
    "homepage": "https://github.com/eliseekn/laravel-metrics",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dieudonné Irié Mohamed",
            "email": "dieudonneirie7@gmail.com",
            "role": "Developer web/mobile"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mdev\\LaravelUrls\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mdev\\LaravelUrls\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "aliases": {
                "LaravelUrls": "Eliseekn\\LaravelUrls\\LaravelUrlsFacade"
            }
        }
    }
}
